{
  "name": "infuse.host",
  "version": "0.0.0",
  "description": "Infuse HTML templates with dynamic content using infuse.host",
  "license": "MIT",
  "author": "Sergio Alcantara <iserg9@gmail.com> (https://github.com/serg-io)",
  "homepage": "https://github.com/serg-io/infuse.host#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serg-io/infuse.host.git"
  },
  "keywords": [
    "infuse",
    "HTML",
    "template",
    "engine"
  ],
  "bugs": {
    "url": "https://github.com/serg-io/infuse.host/issues"
  },
  "main": "src/infuse.js",
  "files": [
    ".babelrc",
    "/lib",
    "/src"
  ],
  "scripts": {
    "lint": "eslint src/*",
    "postversion": "git push && git push --tags && npm publish",
    "prepare": "rm -rf lib && npm run transpile",
    "preversion": "npm run lint && npm run test",
    "test": "jest",
    "transpile": "babel src --out-dir lib --ignore *.test.js"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "domino": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "merge": "^1.2.1"
  }
}